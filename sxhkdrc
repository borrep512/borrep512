# Terminal
super + Return
	kitty

# Cerrar ventana
super + q
	bspc node -c

# Cambiar foco
super + h
	bspc node -f west
super + l
	bspc node -f east
super + j
	bspc node -f south
super + k
	bspc node -f north

# Mover ventana
super + shift + h
	bspc node -s west
super + shift + l
	bspc node -s east
super + shift + j
	bspc node -s south
super + shift + k
	bspc node -s north
